Algoritmo "media"

Var
// Seção de Declarações das variáveis 
  a,b: inteiro
  m : real

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
 escreval("Informe dois numeros  inteiros ")
 leia(a, b)
 
 m <- (a-b)

 escreval("O resultado de ",a," - ",b," é igual a ",m,".")

Fimalgoritmo