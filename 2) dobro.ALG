Algoritmo "media"

Var
// Seção de Declarações das variáveis 
  a: inteiro
  m : real

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
 escreval("Informe dois numeros  inteiros ")
 leia(a)
 
 m <- (a)*2

 escreval("O dobro de  ",a," é igual a ",m,".")

Fimalgoritmo